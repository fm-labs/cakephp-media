{
    "name": "fm-labs/cakephp-media",
    "description": "Media plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=7.1",
        "cakephp/cakephp": "^3.8",
        "fm-labs/cakephp-tree": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Media\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Media\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
