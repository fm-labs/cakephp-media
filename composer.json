{
    "name": "fm-labs/cakephp-media",
    "description": "Media plugin for CakePHP",
    "type": "cakephp-plugin",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/fm-labs/cakephp-tree"
        }
    ],
    "require": {
        "php": ">=8.0",
        "cakephp/cakephp": "^4.4",
        "fm-labs/cakephp-tree": ">=0.2.0",
        "fm-labs/cakephp-upload": "*",
        "imagine/imagine": "^0.7.1|^1.0",
        "league/flysystem": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "suggest": {
        "fm-labs/cakephp-settings": "Settings Plugin for CakePHP"
    },
    "autoload": {
        "psr-4": {
            "Media\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Media\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
