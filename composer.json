{
    "name": "fm-labs/cakephp-media",
    "description": "Media plugin for CakePHP",
    "type": "cakephp-plugin",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/fm-labs/cakephp-tree"
        }
    ],
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "^4.0",
        "fm-labs/cakephp-tree": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Media\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Media\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
